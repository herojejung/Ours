<h1>管理者ページ</h1>
<%= link_to "カテゴリー登録", new_admin_category_path %>
<%= link_to "カテゴリー覧", admin_categories_path %>

<table>
  <thead>
    <tr>
      <th></th>
      <th>タイトル</th>
      <th>ユーザー名</th>
      <th>作成日時</th>
      <th>更新日時</th>
    </tr>
  </thead>
  <tbody>
    <% @post_images.each do |post_image| %>
      <tr>
        <td>
          <%= link_to admin_post_image_path(post_image.id) do%>
          <%= image_tag(post_image.images.first, size: "200x200") %>
          <% end %>
        </td>
        <td><%= post_image.title %></td>
        <td><%= post_image.user.name %></td>
        <td><%= post_image.created_at %></td>
        <td><%= post_image.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @post_images %>